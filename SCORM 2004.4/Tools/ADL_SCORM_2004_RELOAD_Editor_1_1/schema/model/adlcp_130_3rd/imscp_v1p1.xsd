	<!-- ** Complex Types ** -->


	<!-- ******************* -->


	<!-- **************** -->


	<!-- ** dependency ** -->


	<!-- **************** -->

	<xs:complexType name = "dependencyType">
		<xs:sequence>
			<xs:group ref = "grp.any"/>
		</xs:sequence>
		<xs:attributeGroup ref = "attr.identifierref.req"/>
		<xs:anyAttribute namespace = "##other" processContents = "strict"/>
	</xs:complexType>

	<!-- ********** -->


	<!-- ** file ** -->


	<!-- ********** -->

	<xs:complexType name = "fileType">
		<xs:sequence>
			<xs:element ref = "metadata" minOccurs = "0"/>
			<xs:group ref = "grp.any"/>
		</xs:sequence>
		<xs:attributeGroup ref = "attr.href.req"/>
		<xs:anyAttribute namespace = "##other" processContents = "strict"/>
	</xs:complexType>

	<!-- ********** -->


	<!-- ** item ** -->


	<!-- ********** -->

	<xs:complexType name = "itemType">
		<xs:sequence>
			<xs:element ref = "title" minOccurs = "0"/>
			<xs:element ref = "item" minOccurs = "0" maxOccurs = "unbounded"/>
			<xs:element ref = "metadata" minOccurs = "0"/>
			<!-- Start SCORM Declarations -->
			<xs:element ref="adlcp:timeLimitAction" minOccurs="0"/>
			<xs:element ref="adlcp:dataFromLMS" minOccurs="0"/>
			<xs:element ref="adlcp:completionThreshold" minOccurs="0"/>
			<xs:element ref="adlnav:presentation" minOccurs="0"/>
			<xs:element ref="imsss:sequencing" minOccurs="0"/>
			<!-- End SCORM Declarations -->
			<xs:group ref = "grp.any"/>
		</xs:sequence>
		<xs:attributeGroup ref = "attr.identifier.req"/>
		<xs:attributeGroup ref = "attr.identifierref"/>
		<xs:attributeGroup ref = "attr.isvisible"/>
		<xs:attributeGroup ref = "attr.parameters"/>
		<xs:anyAttribute namespace = "##other" processContents = "strict"/>
	</xs:complexType>

	<!-- ************** -->


	<!-- ** manifest ** -->


	<!-- ************** -->

	<xs:complexType name = "manifestType">
		<xs:sequence>
			<xs:element ref = "metadata" minOccurs = "1"/>
			<xs:element ref = "organizations"/>
			<!-- SCORM Declaration -->
			<xs:element ref="imsss:sequencingCollection" minOccurs="0" maxOccurs="1" />
			<xs:element ref = "resources"/>
			<xs:element ref = "manifest" minOccurs = "0" maxOccurs = "unbounded"/>
			<xs:group ref = "grp.any"/>
		</xs:sequence>
		<xs:attributeGroup ref = "attr.identifier.req"/>
		<xs:attributeGroup ref = "attr.version"/>
		<xs:attribute ref = "xml:base"/>
		<xs:anyAttribute namespace = "##other" processContents = "strict"/>
	</xs:complexType>

	<!-- ************** -->


	<!-- ** metadata ** -->


	<!-- ************** -->

	<xs:complexType name = "metadataType">
		<xs:sequence>
			<xs:element ref = "schema" minOccurs = "0"/>
			<xs:element ref = "schemaversion" minOccurs = "0"/>
         	<!-- Start SCORM Declarations -->
         	<xs:element ref="adlcp:location" minOccurs="0"/>
         	<!-- End SCORM Declarations -->
			<xs:group ref = "grp.any"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ******************* -->


	<!-- ** organizations ** -->


	<!-- ******************* -->
	<!-- minOccurs for organization element has been changed from 0 to 1 for model -->
	<!-- to force its addition to a new package (JB) -->
	<xs:complexType name = "organizationsType">
		<xs:sequence>
			<xs:element ref = "organization" minOccurs = "1" maxOccurs = "unbounded"/>
			<xs:group ref = "grp.any"/>
		</xs:sequence>
		<xs:attributeGroup ref = "attr.default"/>
		<xs:anyAttribute namespace = "##other" processContents = "strict"/>
	</xs:complexType>

	<!-- ****************** -->


	<!-- ** organization ** -->


	<!-- ****************** -->

	<xs:complexType name = "organizationType">
		<xs:sequence>
			<xs:element ref = "title" minOccurs = "0"/>
			<xs:element ref = "item" minOccurs = "0" maxOccurs = "unbounded"/>
			<xs:element ref = "metadata" minOccurs = "0"/>
			<xs:element ref="imsss:sequencing" minOccurs="0"/>
			<xs:group ref = "grp.any"/>
		</xs:sequence>
		<xs:attributeGroup ref = "attr.identifier.req"/>
		<xs:attributeGroup ref = "attr.structure.req"/>
		<xs:attributeGroup ref = "attr.objectivesGlobalToSystem"/>
		<xs:anyAttribute namespace = "##other" processContents = "strict"/>
	</xs:complexType>

	<!-- *************** -->


	<!-- ** resources ** -->


	<!-- *************** -->

	<xs:complexType name = "resourcesType">
		<xs:sequence>
			<xs:element ref = "resource" minOccurs = "0" maxOccurs = "unbounded"/>
			<xs:group ref = "grp.any"/>
		</xs:sequence>
		<xs:attributeGroup ref = "attr.base"/>
		<xs:anyAttribute namespace = "##other" processContents = "strict"/>
	</xs:complexType>

	<!-- ************** -->


	<!-- ** resource ** -->


	<!-- ************** -->

	<xs:complexType name = "resourceType">
		<xs:sequence>
			<xs:element ref = "metadata" minOccurs = "0"/>
			<xs:element ref = "file" minOccurs = "0" maxOccurs = "unbounded"/>
			<xs:element ref = "dependency" minOccurs = "0" maxOccurs = "unbounded"/>
			<xs:group ref = "grp.any"/>
		</xs:sequence>
		<xs:attributeGroup ref = "attr.identifier.req"/>
		<xs:attributeGroup ref = "attr.resourcetype.req"/>
		<xs:attributeGroup ref = "attr.base"/>
		<xs:attributeGroup ref = "attr.href"/>
		<!-- Start SCORM Declarations -->
		<xs:attributeGroup ref="attr.scormType" />
		<xs:attributeGroup ref="attr.persistState" />
		<!-- End SCORM Declarations -->
		<xs:anyAttribute namespace = "##other" processContents = "strict"/>
	</xs:complexType>

	<!-- *************** -->


	<!-- ** variation ** -->


	<!-- *************** -->


	<!-- ****************** -->


	<!-- ** Simple Types ** -->


	<!-- ****************** -->

	<xs:simpleType name = "schemaType">
		<xs:restriction base = "xs:string"/>
	</xs:simpleType>
	<xs:simpleType name = "schemaversionType">
		<xs:restriction base = "xs:string"/>
	</xs:simpleType>
	<xs:simpleType name = "titleType">
		<xs:restriction base = "xs:string"/>
	</xs:simpleType>
</xs:schema>